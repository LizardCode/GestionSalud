    <link rel="stylesheet" type="text/css" href="/lib/fullcalendar/main.min.css">
    <link rel="stylesheet" type="text/css" href="/css/Shared/fullcalendar.css">
}

<div class="@ViewBag.MainClass" data-mainclass="@ViewBag.MainClass">

    <div class="row mt-4">
        <div class="col">
            <div class="">
                <div class="card-header p-2 mb-1">
                    <a href="##" onClick="history.go(-1); return false;">
                        <h6 class="card-title"><i class="fa fa-arrow-left"></i> Volver</h6>
                    </a>
                    <h4> Nuevo Sobreturno</h4>
                    <p class="card-title mt-2">Paso 1/2 - Elegí la fecha y hora del sobreturno.</p>
                </div>

                <div class="alert alert-secondary text-center aUpdate" role="alert">
                    <h8>
                        <i class="fa fa-info-circle" style="color: #53b404;"></i> Existe un TURNO agendado.
                        <i class="fa fa-info-circle" style="color: #03a9f4 ;"></i> No existen TURNOS agendados.
                        <i class="fa fa-info-circle" style="color: #ff9800;"></i> Existe un SOBRE-TURNO agendado.
                    </h8>
                </div>

                <div class="col-xl-12 col-xxl-12">
                    <div class="card">
                        <div class="card-body">
                            <div id="AgendaSobreTurnos" class="agendaSobreTurnos"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

