@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" />

    @RenderSection("Metas", required: false)

    <title>@ViewBag.Title - @GlobalSettings.TituloSistema</title>

    <!--https://www.favicon-generator.org/-->
    <link rel="apple-touch-icon" sizes="57x57" href="/img/iconset/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/img/iconset/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/img/iconset/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/img/iconset/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/img/iconset/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/img/iconset/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/img/iconset/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/img/iconset/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/img/iconset/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192" href="/img/iconset/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/img/iconset/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="/img/iconset/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/img/iconset/favicon-16x16.png">
    <link rel="manifest" href="/img/iconset/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="/img/iconset/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <link rel="shortcut icon" href="/img/iconset/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/img/iconset/favicon.ico" type="image/x-icon">

    <link href="https://fonts.googleapis.com/css?family=Quicksand:400,500,600,700&amp;display=swap" rel="stylesheet">

    <link href="~/lib/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/fontawesome/css/all.min.css" rel="stylesheet" />
    <link href="~/lib/animate/animate.min.css" rel="stylesheet" />
    <link href="~/lib/noty/css/noty.css" rel="stylesheet" />
    <link href="~/lib/noty/css/themes/bootstrap-v4.css" rel="stylesheet" />
    <link href="~/lib/datatables/Select-1.3.1/css/select.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/scrollspynav/scrollspyNav.css" rel="stylesheet" />
    <link href="~/lib/sweetalert/ring.css" rel="stylesheet" />
    <link href="/lib/flatpickr/flatpickr.css" rel="stylesheet" type="text/css">
    <link href="/lib/flatpickr/custom-flatpickr.css" rel="stylesheet" type="text/css">
    <link href="~/css/Shared/plugins.css?3661" rel="stylesheet" />
    <link href="~/css/Shared/alert.css?3661" rel="stylesheet" />
    <link href="~/css/Shared/custom-accordions.css?3661" rel="stylesheet" />

    <link href="~/css/Shared/baseLayout.css?3661" rel="stylesheet" />
    <link href="~/css/style.css?3661" rel="stylesheet" />

    <link href="~/css/views/PortalPacientes/portal.css?3661" rel="stylesheet" />

    @RenderSection("Styles", required: false)
</head>

<body class="sidebar-noneoverflow" data-spy="scroll" data-target="#navSection" data-offset="100">

    <div id="load_screen">
        <div class="loader">
            <div class="loader-content">
                <div class="spinner-grow align-self-center"></div>
            </div>
        </div>
    </div>

    <div class="header-container fixed-top">
        <header class="header navbar navbar-expand-sm">
            
            <ul class="navbar-nav theme-brand flex-row  text-center">
                <li class="nav-item theme-logo">
                    <a href="/Home">
                        <img src="/img/portal/logo-salma.png" width="220" class="navbar-logo" alt="logo">
                    </a>
                </li>
                
                <li class="nav-item toggle-sidebar">
                    <a href="javascript:void(0);" class="sidebarCollapse" data-placement="bottom"><i class="fa fa-chevron-left"></i></a>
                </li>
            </ul>

            <ul class="navbar-item flex-row search-ul mr-5">
            </ul>

            <ul class="navbar-item flex-row navbar-dropdown mr-0 mr-md-4">
                <!--USUARIO-->
                <li class="nav-item dropdown message-dropdown">
                    <a href="javascript:void(0);" class="nav-link dropdown-toggle flex-usuario" id="messageDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <img src="/img/default-user.png" class="img-profile" />
                        <div class="altura-flex">
                            <p class="text-white text-usuario">@ViewBag.UsuarioSesion.Nombre</p>
                        </div>
                    </a>
                    <div class="dropdown-menu p-0 position-absolute" aria-labelledby="messageDropdown">
                        <div class="">
                            <a href="@Url.Action("SignOut", "PortalPacientes")">
                                <div class="media cierra-sesion">
                                    <div class="user-img iconos-perfil">
                                        <div class="avatar avatar-xl">
                                            <i class="fa fa-sign-out" aria-hidden="true"></i>
                                        </div>
                                    </div>
                                    <div class="media-body mt-1">
                                        <p class="cierra-sesion-text">Cerrar Sesión</p>
                                    </div>
                                </div>
                            </a>
                        </div>
                    </div>

                </li>
            </ul>

        </header>
    </div>

    <div class="main-container" id="container">

        <div class="overlay"></div>

        <div class="search-overlay"></div>

        <div class="sidebar-wrapper sidebar-theme">
            <nav id="sidebar">
                @Html.Hidden("hidLoggedUser", ViewBag.UsuarioSesion.Id, null)
                @Html.Hidden("hidEmpresa", ViewBag.UsuarioSesion.IdEmpresa, null)
                @Html.Hidden("hidLoggedUserTipo", ViewBag.UsuarioSesion.IdTipoUsuario, null)
                @Html.Hidden("hidLoggedUserIdProfesional", ViewBag.UsuarioSesion.IdProfesional, null)
                @Html.Hidden("hidLoggedUserIdPaciente", ViewBag.UsuarioSesion.IdPaciente, null)
                @{
                    @await Component.InvokeAsync("Menu")
                }
            </nav>
        </div>

        <div id="content" class="main-content">
            <div class="layout-px-spacing">

                @RenderBody()

            </div>

            <div class="footer">
                <span>@Utilities.GetAssemblyVersion("LizardCode.SalmaSalud")</span><br /><img src="~/img/salma-footer.png" width="100">
            </div>
        </div>

    </div>

    <div class="expired-session">
        <div>
            <span>La sesión ha caducado.</span>
            <span>Redirigiendo a la pantalla de inicio de sesión: <span class="counter">5</span></span>
        </div>
    </div>

    <script src="~/lib/jquery/jquery-3.2.1.min.js"></script>
    <script src="~/lib/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/lib/jquery-validation/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/lib/jquery-validation/additional-methods.min.js"></script>
    <script src="~/lib/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/lib/modernizr/modernizr.custom.js"></script>
    <script src="~/lib/moment/moment.min.js"></script>
    <script src="~/lib/feather/feather.js"></script>
    <script src="~/lib/sweetalert/sweetalert.min.js"></script>
    <script src="~/lib/noty/js/noty.min.js"></script>
    <script src="~/lib/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/lib/scrollspynav/scrollspyNav.js"></script>
    <script src="~/lib/fabric/fabric.js"></script>
    <script src="~/lib/autonumeric/autoNumeric.min.js"></script>
    <script src="~/lib/input-mask/jquery.inputmask.bundle.min.js"></script>
    
    <script>
        var RootPath = '@Context.Request.PathBase';
    </script>

    <script src="~/js/Shared/utils.js?3661"></script>
    <script src="~/js/Shared/baseLayout.js?3661"></script>

    <script>
        $.validator.setDefaults({ ignore: ':hidden:not(.validate):not(.validate :hidden), .no-validate' });
        Utils.setupValidator();
    </script>

    @RenderSection("Scripts", required: false)
</body>
</html>
