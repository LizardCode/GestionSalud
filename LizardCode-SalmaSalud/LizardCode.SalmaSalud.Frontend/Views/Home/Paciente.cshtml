@{
    Layout = "~/Views/Shared/BaseLayout.cshtml";

    ViewBag.Title = "";
    ViewBag.MainClass = "DashboardView";
}

@section Styles {
    <link href="/lib/datatables/DataTables-1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="/lib/datatables/Buttons-1.6.1/css/buttons.bootstrap4.min.css" rel="stylesheet" />
    <link href="/lib/select2/css/select2.css" rel="stylesheet" />
    <link href="/lib/datepicker/css/bootstrap-datepicker3.min.css" rel="stylesheet" />
    <link href="/lib/timepicker/css/bootstrap-timepicker.min.css" rel="stylesheet" />
    <link href="/css/Shared/dt-global_style.css?3661" rel="stylesheet" />

    <link href="/css/Shared/maestroLayout.css?3661" rel="stylesheet" />
    <link href="/css/Shared/modal.css?3661" rel="stylesheet" />
    <link href="/css/Views/Home/dashboard.css?3661" rel="stylesheet" />
    <link href="/css/Shared/custom-carousel.css" rel="stylesheet" />
}

<div class="@ViewBag.MainClass" data-mainclass="@ViewBag.MainClass">

    <div class="row layout-top-spacing">

        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 layout-spacing">
            <div class="widget widget-card-four">
                <div class="widget-content">
                    <div class="w-header">
                        <div class="w-info">
                            <div class="row">
                                <div class="col-md-6">
                                    <h6 class="value">Turnos</h6>
                                </div>
                                <div class="col-md-6">
                                    <span class="badge badge-success wBadgeStatus">ATENDIDOS</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="w-content">
                                <div class="w-info">
                                    <img id="AtendidoHoyLoader" alt="" src="/img/mini_loader.gif" width="75px;">
                                    <p class="value text-success pAtendidoHoy" style="display: none;"> [VALUE]<i class="fas fa-info-circle ml-2 fa-info-dashboard" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Hoy"></i></p>

                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="w-content">
                                <div class="w-info">
                                    <img id="AtendidoMensualLoader" alt="" src="/img/mini_loader.gif" width="75px;">
                                    <p class="value text-success pAtendidoMensual" style="display: none;"> [VALUE]<i class="fas fa-info-circle ml-2 fa-info-dashboard" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Mes en Curso"></i></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 layout-spacing">
            <div class="widget widget-card-four">
                <div class="widget-content">
                    <div class="w-header">
                        <div class="w-info">
                            <div class="row">
                                <div class="col-md-6">
                                    <h6 class="value">Turnos</h6>
                                </div>
                                <div class="col-md-6">
                                    <span class="badge badge-primary wBadgeStatus">AGENDADOS</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="w-content">
                                <div class="w-info">
                                    <img id="AgendadoHoyLoader" alt="" src="/img/mini_loader.gif" width="75px;">
                                    <p class="value text-warning pAgendadoHoy" style="display: none;"> [VALUE]<i class="fas fa-info-circle ml-2 fa-info-dashboard" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Hoy"></i></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="w-content">
                                <div class="w-info">
                                    <img id="AgendadoMensualLoader" alt="" src="/img/mini_loader.gif" width="75px;">
                                    <p class="value text-warning pAgendadoMensual" style="display: none;"> [VALUE]<i class="fas fa-info-circle ml-2 fa-info-dashboard" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Mes en Curso"></i></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 layout-spacing">
            <div class="widget widget-card-four">
                <div class="widget-content">
                    <div class="w-header">
                        <div class="w-info">
                            <div class="row">
                                <div class="col-md-6">
                                    <h6 class="value">Turnos</h6>
                                </div>
                                <div class="col-md-6">
                                    <span class="badge badge-danger wBadgeStatus">CANCELADOS</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="w-content">
                                <div class="w-info">
                                    <img id="CanceladoHoyLoader" alt="" src="/img/mini_loader.gif" width="75px;">
                                    <p class="value text-danger pCanceladoHoy" style="display: none;"> [VALUE]<i class="fas fa-info-circle ml-2 fa-info-dashboard" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Hoy"></i></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="w-content">
                                <div class="w-info">
                                    <img id="CanceladoMensualLoader" alt="" src="/img/mini_loader.gif" width="75px;">
                                    <p class="value text-danger pCanceladoMensual" style="display: none;"> [VALUE]<i class="fas fa-info-circle ml-2 fa-info-dashboard" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Mes en Curso"></i></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 layout-spacing">
            <div class="widget widget-card-four">
                <div class="widget-content">
                    <div class="w-header">
                        <div class="w-info">
                            <div class="row">
                                <div class="col-md-6">
                                    <h6 class="value badge badge-warning text-white">Sobre Turnos</h6>
                                </div>
                                <div class="col-md-6">
                                    @*<span class="badge badge-success wBadgeStatus">SOBRETURNOS</span>*@
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="w-content">
                                <div class="w-info">
                                    <img id="SobreTurnosHoyLoader" alt="" src="/img/mini_loader.gif" width="75px;">
                                    <p class="value text-warning pSobreTurnosHoy" style="display: none;"> [VALUE]<i class="fas fa-info-circle ml-2 fa-info-dashboard" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Hoy"></i></p>

                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="w-content">
                                <div class="w-info">
                                    <img id="SobreTurnosMensualLoader" alt="" src="/img/mini_loader.gif" width="75px;">
                                    <p class="value text-warning pSobreTurnosMensual" style="display: none;"> [VALUE]<i class="fas fa-info-circle ml-2 fa-info-dashboard" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Mes en Curso"></i></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

<div id="actionsDialog" class="modal fade actionsDialog" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog bounceInRight animated">
        <div class="modal-content">
            <div id="actionsDialogBody"></div>
        </div>
    </div>
</div>

